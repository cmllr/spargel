
<h3>
<a href="/post/{{ p.id }}/{{ p.slug }}">{{p.title}}</a>
</h3>
<p class="post-meta">
    <span>
        📎 {{p.date | date}}
    </span>
    {% for tag in p.tags %}
        <a href="/?tag={{ tag }}">{{tag}}</a>
    {% endfor %}
</p>
<blockquote>
    <p>
      {{html|safe}}
    </p>
</blockquote> 
<div class="post-meta">

</div>